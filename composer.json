{
    "name": "akas/eloquent-sortable",
    "description": "Sortable behaviour for eloquent models",
    "homepage": "https://github.com/andifahruddinakas/eloquent-sortable",
    "authors": [
        {
            "name": "Andi Fahruddin Akas",
            "email": "andifahruddinakas@gmail.com"
        }
    ],
    "keywords":
        [
            "sort",
            "sortable",
            "eloquent",
            "model",
            "laravel",
            "behaviour"
        ],
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "illuminate/database": "5.8.*|^6|^7|^8|^9",
        "illuminate/support": "5.8.*|^6|^7|^8|^9",
        "nesbot/carbon": "1.*|^2|^3"
    },
    "require-dev": {
        "phpunit/phpunit" : "^8.5",
        "orchestra/testbench": "^3.8|^4.0|^5.0|^6.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Akas\\EloquentSortable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Akas\\EloquentSortable\\Test\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes",
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
